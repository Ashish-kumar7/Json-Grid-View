    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^4.11.4",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@material-ui/styles": "^4.11.4",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "electron-is-dev": "^2.0.0",
    "html-react-parser": "^1.2.6",
    "immutable": "^4.0.0-rc.12",
    "jest-dom": "^4.0.0",
    "js-file-download": "^0.4.12",
    "jsoneditor": "^9.5.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.0",
    "react-data-grid": "^5.0.1",
    "react-data-grid-addons": "^5.0.4-2",
    "react-dom": "^17.0.2",
    "react-json-editor-ajrm": "^2.5.13",
    "react-list-select": "^0.4.1",
    "react-radio-buttons": "^1.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "^4.3.1",
    "react-select-search": "^3.0.7",
    "react-spinners": "^0.11.0",
    "react-split-pane": "^0.1.92",
    "react-test": "^0.8.0",
    "react-test-renderer": "^17.0.2",
    "semantic-ui-react": "^2.0.3",
    "socket.io-client": "^4.1.2",
    "web-vitals": "^1.1.2"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "set BROWSER=none && react-scripts start",
    "start-electron": "electron .",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "preelectron-pack": "yarn build",
    "electron-pack": "build public/electron.js=build/electron.js",
    "electron-dev": "concurrently \"SET BROWSER=none&&npm run start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "electron": "^13.1.2",
    "electron-builder": "^22.11.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "jest": "26.6.0",
    "jest-enzyme": "^7.1.2",
    "mock-prop-types": "^2.1.5",
    "wait-on": "^5.3.0"
  },
  "author": "Abhishek",
  "homepage": "./"
}
